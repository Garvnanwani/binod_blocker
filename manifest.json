{
    "manifest_version": 2,
    "name": "Binod-Blocker",
    "version": "0.1.0",
    "icons": {
        "16": "logo.png",
        "32": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "browser_action": {
        "default_icon": "logo.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["styles.css"]
    }]
}